{
    "chunk_size": 10,
    "positions": {
        "chunks": "top",
        "cards": "top",
        "stack_order": "chunk"
    },
    "visual_options": {
        "highlight_excess": true,
        "auto_hide_text": true,
        "use_good_for_all_pass": false,
        "highlight_perfect": false,
        "perfect_include_hard": true,
        "perfect_color": "#FF3388"
    },
    "chunk_evaluation": {
        "weights": {
            "again": 0.0,
            "hard": 1.0,
            "good": 1.0,
            "easy": 1.0
        },
        "intervals": [
            {
                "enabled": true,
                "start_bracket": "[",
                "start_val": 0.0,
                "end_bracket": ")",
                "end_val": 0.8,
                "color_key": "again",
                "pattern_key": null
            },
            {
                "enabled": true,
                "start_bracket": "[",
                "start_val": 0.8,
                "end_bracket": ")",
                "end_val": 0.85,
                "color_key": "again",
                "pattern_key": "hard"
            },
            {
                "enabled": true,
                "start_bracket": "[",
                "start_val": 0.85,
                "end_bracket": "]",
                "end_val": 0.85,
                "color_key": "hard",
                "pattern_key": null
            },
            {
                "enabled": true,
                "start_bracket": "(",
                "start_val": 0.85,
                "end_bracket": "]",
                "end_val": 0.9,
                "color_key": "hard",
                "pattern_key": "good"
            },
            {
                "enabled": true,
                "start_bracket": "(",
                "start_val": 0.9,
                "end_bracket": "]",
                "end_val": 1.0,
                "color_key": "good",
