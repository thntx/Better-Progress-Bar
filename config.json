                "minutes": false,
                "seconds": true,
                "milliseconds": false
            },
            "style": {
                "color": "#B0B0B0",
                "bold": true,
                "outline": true,
                "outline_color": "#303030"
            },
            "live_enabled": false
        },
        "card_timer": {
            "enabled": false,
            "format": {
                "minutes": false,
