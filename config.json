    "colors": {
        "good": "#66CC66",
        "hard": "#EEDD88",
        "easy": "#AA88DD",
        "again": "#FF6666",
        "excess": "#984B4B",
        "undone": "#404040",
        "buried": "#775544",
        "suspended": "#663333",
        "current": "#66AADD",
        "pending": "#303030"
    },
    "fail_policy": "acknowledge",
    "bury_policy": "acknowledge",
    "suspend_policy": "acknowledge",
    "undo_policy": "undo",
    "double_new": true,
    "text_options": {
        "top": {
            "numbers": {
                "enabled": true,
                "type": "chunks",
                "count_direction": "done",
